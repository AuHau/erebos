{
  "name": "@erebos/client-base",
  "version": "0.8.1",
  "description": "Client base",
  "repository": "git@github.com:MainframeHQ/erebos.git",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "author": "Mainframe",
  "license": "MIT",
  "files": [
    "cjs/*",
    "esm/*",
    "types/*"
  ],
  "scripts": {
    "clean": "del cjs esm",
    "test:types": "tsc --noEmit",
    "build:cjs": "BABEL_ENV='browser-cjs' babel src --out-dir cjs --extensions \".ts\"",
    "build:esm": "BABEL_ENV='browser-esm' babel src --out-dir esm --extensions \".ts\"",
    "build:js": "yarn build:cjs && yarn build:esm",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn test:types && yarn clean && yarn build:js && yarn build:types",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@erebos/api-pss": "^0.7.0",
    "@mainframe/rpc-base": "^0.3.0"
  },
  "peerDependencies": {
    "@erebos/api-bzz-base": "^0.8.1"
  },
  "devDependencies": {
    "@erebos/api-bzz-base": "^0.8.1",
    "typescript": "^3.5.2"
  }
}
